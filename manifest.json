{
    "manifest_version": 3,
    "name": "HinaEPSTools",
    "version": "0.1.1",
    "description": "",
    "permissions": [
        "activeTab",
        "declarativeContent",
        "scripting",
        "storage",
        "cookies"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}